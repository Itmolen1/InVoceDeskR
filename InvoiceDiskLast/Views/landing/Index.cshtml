<script type="text/javascript">
    $(document).ready(function () {
       
        $('#btnSubmit').click(function () {
            
            if (!Validation()) {
                return;
            }
            else {
                $('#btnSubmit').text('saving...');
                postMessage();
            }
        });
    });

    function postMessage() {

        var feedbackData = {
            Name: $('#txtName').val(),
            Email: $('#txtEmail').val(),
            Subject: $('#txtSubject').val(),
            Message: $('#txtMessage').val(),
        }
        $.ajax({
            url: "/landing/FeedBack",
            type: "POST",
            data: JSON.stringify(feedbackData),
            contentType: "Application/json",
            dataType: "json",
            success: function (response) {
                $('#btnSubmit').text('Send Message');
                Clear();
                $('#succss').css('visibility', 'visible');
            },
            error: function (Error) {

            }
        });
    }


    function Clear() {

        $('#txtName').val('');
        $('#txtEmail').val('');
        $('#txtSubject').val('');
        $('#txtMessage').val('');

    }


    function Validation() {

        var isValid = true;

        if ($('#txtName').val().trim() == "") {
            $('#txtName').css('border-color', 'Red');

            isValid = false;
        }
        else {
            $('#txtName').css('border-color', 'lightgrey');
        }
        if ($('#txtName').val().trim() == "") {
            $('#txtName').css('border-color', 'Red');
            isValid = false;
        }
        else {
            $('#txtName').css('border-color', 'lightgrey');
        }
        if ($('#txtEmail').val().trim() == "") {
            $('#txtEmail').css('border-color', 'Red');
            $('#txtEmail').focus();
            isValid = false;
        }
        else {
            $('#txtEmail').css('border-color', 'lightgrey');
        }
        if ($('#txtSubject').val() == "") {
            $('#txtSubject').css('border-color', 'Red');
            isValid = false;

        }
        else {
            $('#txtSubject').css('border-color', 'lightgrey');
        }


        if ($('#txtMessage').val() == "") {
            $('#txtMessage').css('border-color', 'Red');
            isValid = false;
            return;

        }
        else {
            $('#txtMessage').css('border-color', 'lightgrey');
        }

        var userinput = $('#txtEmail').val();
        var expr = /^([\w-\.]+)(\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+)([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;

        if (!expr.test(userinput)) {
            isValid = false;
            alert('not a valid e-mail address');
        }

        return isValid;

    }

</script>