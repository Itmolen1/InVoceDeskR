            type: "POST",
            async: false,
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            data: data,
            success: function (data) {
                if (data.Status == "Success") {


                    swal({
                        title: 'Bill!',
                        text: 'Row deleted successfully!',
                        type: 'success'
                    });

                }
                else if (data.Status == "Fail") {

                    swal({
                        title: 'Invoice!',
                        text: 'Fialied to delete row!',
                        type: 'danger'
                    });

                }
            },
            Error: function (errormessage) {
                alert(errormessage);
            }

        });

    }




</script>


