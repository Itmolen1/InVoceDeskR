


@if (ViewBag.FILE != null)
{
    <div class="row">
        <div class="col-8 b-r">
            <table class="table table-hover" id="participantTable">
                <tbody id="upload">

                        @foreach (var item in ViewBag.FILE)
                        {
                            <tr>

                                <td>
                                    <input type='text' class='form-control FileName' value="@item.DirectoryPath" name='FileName' readonly>
                                </td>
                                <td>
                                    <button class='btn btn-danger col-sm btn-sm remove' type='button'><i class='fa fa-trash'></i></button>
                                </td>
                               
                            </tr>
                        }

      
                </tbody>
            </table>
        </div>
        <div class="col-4 b-r"></div>
    </div>
}


<div class="row">
    <div class="col-12 b-r">
        <form name='frmAccount' id='frmAccount'>

            <input type='file' name='file23' multiple id="uploadfile" class="makethispretty btn btn-success" />

        </form>
    </div>

</div>


